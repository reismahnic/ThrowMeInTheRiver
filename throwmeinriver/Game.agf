        <SpeechLines>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I see nothing special about it." Location="GlobalScript.asc" Function="unhandled_event" LineNum="65" Type="GLOBAL" SpeechFunction="Say" BlockNumber="6.0.0" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I can't do anything with it." Location="GlobalScript.asc" Function="unhandled_event" LineNum="69" Type="GLOBAL" SpeechFunction="Say" BlockNumber="6.0.1" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="That won't do anything." Location="GlobalScript.asc" Function="unhandled_event" LineNum="73" Type="GLOBAL" SpeechFunction="Say" BlockNumber="6.0.2" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Looks alright." Location="GlobalScript.asc" Function="unhandled_event" LineNum="80" Type="GLOBAL" SpeechFunction="Say" BlockNumber="6.1.0" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I don't want to have it." Location="GlobalScript.asc" Function="unhandled_event" LineNum="84" Type="GLOBAL" SpeechFunction="Say" BlockNumber="6.1.1" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="That's a funny idea." Location="GlobalScript.asc" Function="unhandled_event" LineNum="88" Type="GLOBAL" SpeechFunction="Say" BlockNumber="6.1.2" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Hm." Location="GlobalScript.asc" Function="unhandled_event" LineNum="95" Type="GLOBAL" SpeechFunction="Say" BlockNumber="6.2.0" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Got nothing to say." Location="GlobalScript.asc" Function="unhandled_event" LineNum="99" Type="GLOBAL" SpeechFunction="Say" BlockNumber="6.2.1" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I don't think I should give that away." Location="GlobalScript.asc" Function="unhandled_event" LineNum="103" Type="GLOBAL" SpeechFunction="Say" BlockNumber="6.2.2" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="It's just some junk in my inventory." Location="GlobalScript.asc" Function="unhandled_event" LineNum="110" Type="GLOBAL" SpeechFunction="Say" BlockNumber="6.3.0" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Er, no?" Location="GlobalScript.asc" Function="unhandled_event" LineNum="114" Type="GLOBAL" SpeechFunction="Say" BlockNumber="6.3.1" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="That's ridiculous." Location="GlobalScript.asc" Function="unhandled_event" LineNum="118" Type="GLOBAL" SpeechFunction="Say" BlockNumber="6.3.2" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Just jerky enough." Location="GlobalScript.asc" Function="iBeefJerky_Look" LineNum="155" Type="GLOBAL" SpeechFunction="Say" BlockNumber="11" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="You sure see them a lot these days!" Location="GlobalScript.asc" Function="iBlueCup_Look" LineNum="161" Type="GLOBAL" SpeechFunction="Say" BlockNumber="12" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="It's for holding coffee, or tea, or MAYBE cocoa. Nothing else." Location="GlobalScript.asc" Function="iBlueCup_UseInv" LineNum="166" Type="GLOBAL" SpeechFunction="Say" BlockNumber="13" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I just started to learn how to play them." Location="GlobalScript.asc" Function="iPipes_Look" LineNum="175" Type="GLOBAL" SpeechFunction="Say" BlockNumber="14" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Pfeeeeeeeehhhhppppphrrrrrfff..." Location="GlobalScript.asc" Function="iPipes_Interact" LineNum="180" Type="GLOBAL" SpeechFunction="Say" BlockNumber="15" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I was born on a farm in Alberta, along the Rockies." Location="room2.asc" Function="room_AfterFadeIn" LineNum="5" Type="ROOM" SpeechFunction="Say" BlockNumber="0" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="My mother passed when I was still to young to work, and my father cared for my sister and I." Location="room2.asc" Function="room_AfterFadeIn" LineNum="6" Type="ROOM" SpeechFunction="Say" BlockNumber="0" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="When I was eight years old, I experienced somthing very odd." Location="room2.asc" Function="room_AfterFadeIn" LineNum="7" Type="ROOM" SpeechFunction="Say" BlockNumber="0" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I was running through the high grass near the river, when a bright light illuminated the sky." Location="room2.asc" Function="room_AfterFadeIn" LineNum="8" Type="ROOM" SpeechFunction="Say" BlockNumber="0" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="It flashed twice, then slowly descended, until it fell into my outstretched palm." Location="room2.asc" Function="room_AfterFadeIn" LineNum="9" Type="ROOM" SpeechFunction="Say" BlockNumber="0" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I experienced something that I do not understand." Location="room2.asc" Function="room_AfterFadeIn" LineNum="10" Type="ROOM" SpeechFunction="Say" BlockNumber="0" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="What do you think it was?" Location="Dialog 0" Function="Start" LineNum="4" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="At the time I thought it was some kind of greater power, calling down to me." Location="Dialog 0" Function="Start" LineNum="5" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="And now?" Location="Dialog 0" Function="Start" LineNum="6" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I still believe it may have been God." Location="Dialog 0" Function="Option 1" LineNum="14" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I think it was some kind of higher power. Talking to me. Telling me I would be ok." Location="Dialog 0" Function="Option 1" LineNum="15" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="You've never told me about this." Location="Dialog 0" Function="Option 1" LineNum="16" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I used to avoid thinking about it." Location="Dialog 0" Function="Option 1" LineNum="17" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I like it, Dad." Location="Dialog 0" Function="Option 1" LineNum="18" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I don't know what it was. But I don't believe in a higher power." Location="Dialog 0" Function="Option 2" LineNum="24" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I believe you saw something, though." Location="Dialog 0" Function="Option 2" LineNum="25" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="More and more I think about it, the more I think I just saw what I needed to see, and that's all there was to it." Location="Dialog 0" Function="Option 2" LineNum="26" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Something that would get me as far as possible from..." Location="Dialog 0" Function="Option 2" LineNum="27" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 0" Function="Option 2" LineNum="28" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="It was lonely out there. Just had my sister." Location="Dialog 0" Function="Option 2" LineNum="29" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Aunt Cheryl talks enough for four people." Location="Dialog 0" Function="Option 2" LineNum="30" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Heh! Isn't that the truth..." Location="Dialog 0" Function="Option 2" LineNum="31" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="It was God" Location="Dialog 0" Function="Option 1" LineNum="12" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="It was not God" Location="Dialog 0" Function="Option 2" LineNum="22" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 1" Function="Start" LineNum="4" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 1" Function="Start" LineNum="5" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I'm probably not leaving this hospital, Diane." Location="Dialog 1" Function="Start" LineNum="6" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Dad..." Location="Dialog 1" Function="Start" LineNum="7" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 1" Function="Start" LineNum="8" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Do you remember when you were in high school, and I was in a car wreck?" Location="Dialog 1" Function="Start" LineNum="9" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Yes." Location="Dialog 1" Function="Start" LineNum="10" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I was going... had to be 150, 160. Just outside of Toronto." Location="Dialog 1" Function="Start" LineNum="11" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Your Mom had left me and I didn't know what to do." Location="Dialog 1" Function="Start" LineNum="12" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I remember." Location="Dialog 1" Function="Start" LineNum="13" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="And I remember dangling upside down, and struggling to get my seatbelt unhooked..." Location="Dialog 1" Function="Start" LineNum="14" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="And then I stopped, and just hung there. I gave up." Location="Dialog 1" Function="Start" LineNum="15" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I thought, please, God, come to me, speak to me like you spoke to me that day on the farm." Location="Dialog 1" Function="Start" LineNum="18" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I didn't know what to do. I was to scared to survive, and not religious enough to pray." Location="Dialog 1" Function="Start" LineNum="22" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 1" Function="Start" LineNum="24" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 1" Function="Start" LineNum="25" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I'm glad I lived." Location="Dialog 1" Function="Start" LineNum="26" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I know." Location="Dialog 1" Function="Start" LineNum="27" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I'm glad you're here. Right now, I mean." Location="Dialog 1" Function="Start" LineNum="28" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 1" Function="Start" LineNum="29" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I know." Location="Dialog 1" Function="Start" LineNum="30" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 1" Function="Start" LineNum="31" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="How are things at home?" Location="Dialog 1" Function="Start" LineNum="32" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="What's Paul up to lately?" Location="Dialog 1" Function="Option 1" LineNum="39" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="He's taking retirement hard." Location="Dialog 1" Function="Option 1" LineNum="40" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I did too, at first." Location="Dialog 1" Function="Option 1" LineNum="41" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="He's been seeing his friends more, though." Location="Dialog 1" Function="Option 1" LineNum="42" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="From work?" Location="Dialog 1" Function="Option 1" LineNum="43" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Mostly. Sometimes his best friend he grew up with visits." Location="Dialog 1" Function="Option 1" LineNum="44" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="That's good." Location="Dialog 1" Function="Option 1" LineNum="45" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Are Liz and Emma doing ok?" Location="Dialog 1" Function="Option 2" LineNum="48" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Liz is doing fine. She's been looking up and down for colleges." Location="Dialog 1" Function="Option 2" LineNum="49" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Oh, yeah. What's she majoring in?" Location="Dialog 1" Function="Option 2" LineNum="50" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="She doesn't know yet. Just getting her general degree." Location="Dialog 1" Function="Option 2" LineNum="51" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Emma, of course, getting straight A's in everything. You know her." Location="Dialog 1" Function="Option 2" LineNum="52" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Well, I'd like to see both of them." Location="Dialog 1" Function="Option 2" LineNum="53" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I know, Dad. They're just both so busy now..." Location="Dialog 1" Function="Option 2" LineNum="54" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I'm fine." Location="Dialog 1" Function="Option 3" LineNum="58" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="You've been coming here too often, Diane." Location="Dialog 1" Function="Option 3" LineNum="59" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I said I'm fine, Dad." Location="Dialog 1" Function="Option 3" LineNum="60" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Because I can-" Location="Dialog 1" Function="Option 3" LineNum="61" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Dad." Location="Dialog 1" Function="Option 3" LineNum="62" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="You're fine." Location="Dialog 1" Function="Option 3" LineNum="63" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I'm fine." Location="Dialog 1" Function="Option 3" LineNum="64" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="How's Paul doing?" Location="Dialog 1" Function="Option 1" LineNum="37" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="How are Liz and Emma?" Location="Dialog 1" Function="Option 2" LineNum="47" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="How have you been?" Location="Dialog 1" Function="Option 3" LineNum="57" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 2" Function="Start" LineNum="4" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Diane?" Location="Dialog 2" Function="Start" LineNum="5" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="What?" Location="Dialog 2" Function="Start" LineNum="6" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="When I go, I want them there." Location="Dialog 2" Function="Start" LineNum="7" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="For the funeral." Location="Dialog 2" Function="Start" LineNum="8" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Of course they'll be there, Dad." Location="Dialog 2" Function="Start" LineNum="9" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I'll never forgive myself for what I did." Location="Dialog 2" Function="Option 1" LineNum="15" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I was angry, but missing my own father's funeral..." Location="Dialog 2" Function="Option 1" LineNum="16" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="It's alright. I'm sure he knew you cared about him." Location="Dialog 2" Function="Option 1" LineNum="17" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="It was disgusting. I was a coward." Location="Dialog 2" Function="Option 1" LineNum="18" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="And I'll pay for it, when the time comes." Location="Dialog 2" Function="Option 1" LineNum="21" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="That wasn't a sin, Dad." Location="Dialog 2" Function="Option 1" LineNum="22" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Of course it was." Location="Dialog 2" Function="Option 1" LineNum="23" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="You can't let go of that." Location="Dialog 2" Function="Option 1" LineNum="27" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Burying my father was..." Location="Dialog 2" Function="Option 2" LineNum="33" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="He and I never got closure. At all. I hadn't seen him since I left home." Location="Dialog 2" Function="Option 2" LineNum="34" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I know. Mom told me." Location="Dialog 2" Function="Option 2" LineNum="35" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Well, I'll never feel good about our relationship." Location="Dialog 2" Function="Option 2" LineNum="36" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="But I know that Cheryl appreciated it. And that's why I went." Location="Dialog 2" Function="Option 2" LineNum="37" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I'm glad I went." Location="Dialog 2" Function="Option 2" LineNum="38" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="God only knows if the old man would've wanted me there." Location="Dialog 2" Function="Option 2" LineNum="41" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I still don't know if the old man wouldn've wanted me there." Location="Dialog 2" Function="Option 2" LineNum="45" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Anyway, I want you there for mine." Location="Dialog 2" Function="Option 2" LineNum="47" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I will be, Dad." Location="Dialog 2" Function="Option 2" LineNum="48" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I didn't go to my father's funeral" Location="Dialog 2" Function="Option 1" LineNum="13" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I went to my father's funeral" Location="Dialog 2" Function="Option 2" LineNum="31" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="You've never talked about him before. Not like this." Location="Dialog 3" Function="Start" LineNum="4" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I'm at the point where I won't get to later." Location="Dialog 3" Function="Start" LineNum="5" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Nobody deserves to be completely forgotten. Not even him." Location="Dialog 3" Function="Start" LineNum="6" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="What... what happened between you two?" Location="Dialog 3" Function="Start" LineNum="7" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="He wasn't what you'd call a loving parent." Location="Dialog 3" Function="Start" LineNum="8" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="After my mother died, I became the target of his grief." Location="Dialog 3" Function="Start" LineNum="9" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="One day I had to stand up to him. And I did." Location="Dialog 3" Function="Start" LineNum="10" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="The next day, I left home. And I never saw him again." Location="Dialog 3" Function="Start" LineNum="11" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Until the funeral." Location="Dialog 3" Function="Start" LineNum="14" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 3" Function="Start" LineNum="18" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="When you left... did you hate him?" Location="Dialog 3" Function="Start" LineNum="20" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Oh, I sure did." Location="Dialog 3" Function="Option 1" LineNum="26" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I hated him like no other." Location="Dialog 3" Function="Option 1" LineNum="27" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="And even when I was forty, and heard he died, I still hated him." Location="Dialog 3" Function="Option 1" LineNum="30" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Part of me still hates him. And the other part of me is just sad." Location="Dialog 3" Function="Option 1" LineNum="31" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="What's happened happened. Can't do anything about it." Location="Dialog 3" Function="Option 1" LineNum="32" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I wish I'd gone to his funeral..." Location="Dialog 3" Function="Option 1" LineNum="33" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="But by the time he died, I was forty years old, and realized I would never understand him." Location="Dialog 3" Function="Option 1" LineNum="37" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Part of me still hates him. And the other part of me is just sad." Location="Dialog 3" Function="Option 1" LineNum="38" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="What's happened happened. Can't do anything about it." Location="Dialog 3" Function="Option 1" LineNum="39" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I'm glad I went to his funeral." Location="Dialog 3" Function="Option 1" LineNum="40" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Thank you for telling me about him." Location="Dialog 3" Function="Option 1" LineNum="43" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I should have a long time ago." Location="Dialog 3" Function="Option 1" LineNum="44" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I'm sorry." Location="Dialog 3" Function="Option 1" LineNum="45" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 3" Function="Option 1" LineNum="46" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="No. I was angry, but... I knew I didn't understand." Location="Dialog 3" Function="Option 2" LineNum="52" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I would never understand him. We were just too different." Location="Dialog 3" Function="Option 2" LineNum="53" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Growing up, he'd tell me how I was just like my mother." Location="Dialog 3" Function="Option 2" LineNum="54" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="And he was right, I was." Location="Dialog 3" Function="Option 2" LineNum="55" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I couldn't live with him anymore. So I left." Location="Dialog 3" Function="Option 2" LineNum="56" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="It's good that you went to the funeral." Location="Dialog 3" Function="Option 2" LineNum="59" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Yes, it was." Location="Dialog 3" Function="Option 2" LineNum="60" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Why didn't you go to the funeral?" Location="Dialog 3" Function="Option 2" LineNum="64" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I left him and never came back. How could I?" Location="Dialog 3" Function="Option 2" LineNum="65" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Thank you for telling me about him." Location="Dialog 3" Function="Option 2" LineNum="68" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I should have a long time ago." Location="Dialog 3" Function="Option 2" LineNum="69" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I'm sorry." Location="Dialog 3" Function="Option 2" LineNum="70" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 3" Function="Option 2" LineNum="71" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I hated him" Location="Dialog 3" Function="Option 1" LineNum="24" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="No, I didn't hate him" Location="Dialog 3" Function="Option 2" LineNum="50" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Dad, you know, when I divorced Eric?" Location="Dialog 4" Function="Start" LineNum="4" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I remember. I didn't understand." Location="Dialog 4" Function="Option 1" LineNum="9" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Well... I was furious with you." Location="Dialog 4" Function="Option 1" LineNum="10" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I know." Location="Dialog 4" Function="Option 1" LineNum="11" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Just like when you hated my Grandfather." Location="Dialog 4" Function="Option 1" LineNum="14" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I know." Location="Dialog 4" Function="Option 1" LineNum="15" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="After you and Mom divorced, for you to say..." Location="Dialog 4" Function="Option 1" LineNum="16" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="It was stupid. And I still don't understand why I said what I said." Location="Dialog 4" Function="Option 1" LineNum="17" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I'm sorry." Location="Dialog 4" Function="Option 1" LineNum="18" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Don't be. It was twenty years ago. I'm past it." Location="Dialog 4" Function="Option 1" LineNum="19" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I just... I understand how you felt. When you left your father." Location="Dialog 4" Function="Option 1" LineNum="20" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I didn't hate you though." Location="Dialog 4" Function="Option 1" LineNum="24" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Well, you're like me. You always have been." Location="Dialog 4" Function="Option 1" LineNum="25" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="You couldn't hate your father, I can't hate mine." Location="Dialog 4" Function="Option 1" LineNum="26" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I don't understand how it works. But I don't mind." Location="Dialog 4" Function="Option 1" LineNum="27" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I remember when you and Mom got divorced, and I felt like my life was being torn apart." Location="Dialog 4" Function="Option 2" LineNum="31" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Well, sure. You know, when I went through my divorce, we got a lot of comments from our church." Location="Dialog 4" Function="Option 2" LineNum="34" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="It was '73. People gave us a hard time." Location="Dialog 4" Function="Option 2" LineNum="35" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Well, sure. My divorce was no party. I understood." Location="Dialog 4" Function="Option 2" LineNum="39" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Your understanding was what got me through all of that." Location="Dialog 4" Function="Option 2" LineNum="42" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I have to wonder if you and my Grandfather could have had a relationship, if you had tried to be more understanding." Location="Dialog 4" Function="Option 2" LineNum="46" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Do you think you and my Grandfather could have worked things out?" Location="Dialog 4" Function="Option 2" LineNum="50" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="You say there wasn't understanding, even though you didn't hate him." Location="Dialog 4" Function="Option 2" LineNum="51" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Do you think you could have found understanding?" Location="Dialog 4" Function="Option 2" LineNum="52" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I don't know." Location="Dialog 4" Function="Option 2" LineNum="55" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="That's mine, hold on." Location="Dialog 4" Function="Option 2" LineNum="59" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 4" Function="Option 2" LineNum="60" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Hey, Emma." Location="Dialog 4" Function="Option 2" LineNum="61" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 4" Function="Option 2" LineNum="62" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="What time?" Location="Dialog 4" Function="Option 2" LineNum="63" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 4" Function="Option 2" LineNum="64" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 4" Function="Option 2" LineNum="65" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Alright, I'll be there soon." Location="Dialog 4" Function="Option 2" LineNum="66" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="..." Location="Dialog 4" Function="Option 2" LineNum="67" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Ok. See ya." Location="Dialog 4" Function="Option 2" LineNum="68" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Gotta run?" Location="Dialog 4" Function="Option 2" LineNum="69" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Yes. But I'll be back tomorrow." Location="Dialog 4" Function="Option 2" LineNum="70" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Diane..." Location="Dialog 4" Function="Option 2" LineNum="71" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Dad?" Location="Dialog 4" Function="Option 2" LineNum="72" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Thank you." Location="Dialog 4" Function="Option 2" LineNum="73" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Of course, Dad. I'll see you soon." Location="Dialog 4" Function="Option 2" LineNum="74" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Drive safe." Location="Dialog 4" Function="Option 2" LineNum="75" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="-1">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="Soon..." Location="Dialog 4" Function="Option 2" LineNum="77" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I didn't approve" Location="Dialog 4" Function="Option 1" LineNum="8" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
          <Speech CharacterID="1000">
            <ScriptLines>
              <ScriptLine Text="" OriginalText="I was happy for you" Location="Dialog 4" Function="Option 2" LineNum="30" Type="DIALOG" SpeechFunction="N/A" BlockNumber="" />
            </ScriptLines>
          </Speech>
